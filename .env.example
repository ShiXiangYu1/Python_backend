# 应用程序设置
APP_NAME=AI模型管理与服务平台
APP_ENV=development  # development, production, testing
APP_DEBUG=true
APP_PORT=8000
APP_HOST=0.0.0.0
SECRET_KEY=your_secret_key_here
API_PREFIX=/api

# 数据库设置
DB_CONNECTION=mysql
DB_HOST=localhost
DB_PORT=3306
DB_DATABASE=ai_model_platform
DB_USERNAME=root
DB_PASSWORD=password

# Redis设置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# Elasticsearch设置
ES_HOST=localhost
ES_PORT=9200
ES_USERNAME=
ES_PASSWORD=

# JWT设置
JWT_SECRET_KEY=your_jwt_secret_key_here
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# 模型设置
MODEL_UPLOAD_DIR=./model_uploads
MAX_MODEL_SIZE=1073741824  # 1GB in bytes

# 日志设置
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_DIR=./logs

# CORS设置
CORS_ALLOW_ORIGINS=http://localhost:3000,http://localhost:8000

# 线程池设置
WORKER_POOL_SIZE=4 